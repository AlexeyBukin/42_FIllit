#include "test.h"

int		main(int argc, char **argv)
{
	char	*s;
	int		i;

	if (argc == 1)
	{
		s = "To be or not to be";
		printf("ft_strlen(\'%s\'): %zu\n", s, ft_strlen(s));
		return (0);
	}
	i = 1;
	while (i < argc)
	{
		printf("ft_strlen(\'%s\'): %zu\n", argv[i], ft_strlen(argv[i]));
		i++;
	}
	return (0);
}
